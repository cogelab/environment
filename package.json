{
  "name": "coge-environment",
  "version": "0.8.0",
  "description": "Handles lookups of generators in a specific environment",
  "license": "MIT",
  "repository": "https://github.com/cogelab/coge-environment",
  "author": "",
  "keywords": [
    "development",
    "dev",
    "build",
    "tool",
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "coge",
    "app"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prerelease": "cross-env CI=1 npm run test",
    "release": "np --no-2fa",
    "build": "lb-tsc -b",
    "build:full": "npm ci --ignore-scripts && npm run clean && npm run build",
    "precoverage": "npm test",
    "coverage": "lb-nyc report --reporter=text-summary",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "lb-eslint --report-unused-disable-directives --cache .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "npm run prettier:cli -- --check",
    "prettier:fix": "npm run prettier:cli -- --write",
    "format": "npm run prettier:fix",
    "clean": "lb-clean dist coverage .nyc_output *.tsbuildinfo",
    "pretest": "npm run clean && npm run format && npm run build",
    "test": "lb-nyc jest",
    "test:ci": "lb-nyc jest",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@loopx/utils": "^2.6.0",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.1",
    "debug": "^4.3.1",
    "diff": "^5.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "inquirer": "^8.1.0",
    "is-scoped": "^2.1.0",
    "log-symbols": "^4.1.0",
    "semver": "^7.3.5",
    "strip-ansi": "^6.0.0",
    "text-table": "^0.2.0",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.4.0",
    "@loopback/eslint-config": "^10.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.1",
    "@types/sinon": "^10.0.2",
    "@types/text-table": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "cross-env": "^7.0.3",
    "dir-compare": "^3.3.0",
    "jest": "^27.0.4",
    "np": "^7.5.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
